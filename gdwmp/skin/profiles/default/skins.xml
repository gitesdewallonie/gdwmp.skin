<?xml version="1.0"?>
<object name="portal_skins" allow_any="False" cookie_persistence="False"
   default_skin="GdwMPSkin">

 <object name="gdwmp_skin_images"
    meta_type="Filesystem Directory View"
    directory="gdwmp.skin:skins/gdwmp_skin_images"/>
 <object name="gdwmp_skin_styles"
    meta_type="Filesystem Directory View"
    directory="gdwmp.skin:skins/gdwmp_skin_styles"/>
 <object name="gdwmp_skin_templates"
    meta_type="Filesystem Directory View"
    directory="gdwmp.skin:skins/gdwmp_skin_templates"/>
 <object name="gdwmp_skin_scripts"
    meta_type="Filesystem Directory View"
    directory="gdwmp.skin:skins/gdwmp_skin_scripts"/>

 <skin-path name="GdwMPSkin" based-on="Sunburst Theme">
  <layer name="gdwmp_skin_images"
     insert-after="custom"/>
  <layer name="gdwmp_skin_styles"
     insert-after="gdwmp_skin_images"/>
  <layer name="gdwmp_skin_templates"
     insert-after="gdwmp_skin_styles"/>
  <layer name="gdwmp_skin_scripts"
     insert-after="gdwmp_skin_templates"/>
 </skin-path>

</object>
